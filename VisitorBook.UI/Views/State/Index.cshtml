<div class="container">
    <h1 class="text-center">@localization["States.Title.Text"]</h1>
    <div>
        <div class="float-start" id="outside">
        </div>
        <div class="float-end">
            <a onclick="showInPopup('@Url.Action("AddOrEdit", "State", new { Id = 0 }, Context.Request.Scheme)', '@localization["States.AddStateBtn.Text"]')"
               class="btn btn-primary mb-3 float-end" role='button'>
                @localization["States.AddStateBtn.Text"]
            </a>
        </div>
    </div>
    <table id="tblData" class="table table-bordered table-striped" style="width: 100%">
        <thead>
            <tr>
                <th>
                    @localization["States.Table.Column1.Text"]
                </th>
                <th>
                    @localization["States.Table.Column2.Text"]
                </th>
                <th>
                    @localization["States.Table.Column3.Text"]
                </th>
                <th>
                    @localization["States.Table.Column4.Text"]
                </th>
                <th>

                </th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/state.js"></script>
}
