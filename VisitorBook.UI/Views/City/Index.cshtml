<div class="container">
    <h1 class="text-center">City List</h1>
    <div>
        <div class="float-start" id="outside">

        </div>
        <div class="float-end">
            <a onclick="showInPopup('@Url.Action("AddOrEdit", "City", new { Id = 0 }, Context.Request.Scheme)', 'Add City')"
               class="btn btn-primary mb-3 float-end" role='button'>
                Add City
            </a>
        </div>
    </div>
    <table id="tblData" class="table table-bordered table-striped" style="width: 100%">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Code
                </th>
                <th>

                </th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/city.js"></script>
}
